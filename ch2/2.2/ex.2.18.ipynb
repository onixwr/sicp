    "\n",
    "(define (reverse1 items)\n",
    "  (define (itr reversed-items items)\n",
    "    (if (null? items)\n",
    "      reversed-items\n",
    "      (itr (cons (car items) reversed-items) (cdr items))\n",
    "    )\n",
    "  )\n",
    "  (itr '() items)\n",
    ")\n"
